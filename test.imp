( sito Eratostenesa )
(
VAR
    a b
BEGIN
    READ a;
    WHILE a > 0 DO
        b := a / 2;
        b := 2 * b;
        IF a > b THEN WRITE 1;
        ELSE WRITE 0;
        ENDIF
        a := a /2;
    ENDWHILE
END
)

( sito Eratostenesa )
VAR
n j sito [100]
BEGIN
n := 100-1;
FOR i FROM n DOWNTO 2 DO
sito [i] := 1;
ENDFOR
FOR i FROM 2 TO n DO
IF sito [i] <> 0 THEN
j := i + i;
WHILE j <= n DO
sito [j] := 0;
j := j + i;
ENDWHILE
WRITE i;
ENDIF
ENDFOR
END